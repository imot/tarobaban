<launch>
	<arg name="model" />
	<arg name="gui" default="False" />
	<param name="robot_description" textfile="$(arg model)" />
	<param name="use_gui" value="$(arg gui)"/>
	<node name="talker" pkg="tarobaban" type="talker.py" />
	<node name="joy_marker" pkg="tarobaban" type="joy_marker.py" />
	<node name="joy_call" pkg="tarobaban" type="joy_call.py" />
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find tarobaban)/urdf.rviz" required="true" />
</launch>
